{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "check-ts": "tsc --noEmit",
    "build:spa:nocheck": "cross-env NODE_ENV=production vite build",
    "build:spa": "npm run check-ts && npm run build:spa:nocheck",
    "build:ssr:nocheck": "cross-env BUILD_MODE=ssr NODE_ENV=production vite build --outDir=distSSR",
    "build:ssr": "npm run check-ts && npm run build:ssr:nocheck",
    "build:packages": "npm run check-ts && npm run build:ssr:nocheck && npm run build:spa:nocheck",
    "compress-artifacts": "node scripts/comp.js --formats=gzip",
    "copy-licenses": "node scripts/copyLicenses.js",
    "prerender:es": "node distSSR/ssr.es.js",
    "prerender": "node distSSR/ssr.cjs.js",
    "build": "npm run build:packages && npm run prerender && npm run copy-licenses && npm run compress-artifacts",
    "serve": "vite preview",
    "subset": "node subset.mjs",
    "font-comp": "node font"
  },
  "dependencies": {
    "@svelte-navigator/history": "^0.4.0",
    "classnames": "^2.3.1",
    "preact": "^10.5.13"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@squoosh/cli": "^0.6.4",
    "@types/css-declaration-sorter": "^5.1.0",
    "@types/html-minifier-terser": "^5.1.1",
    "@types/minimatch": "^3.0.4",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^6.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-minifier-terser": "^5.1.1",
    "minimatch": "^3.0.4",
    "postcss": "^8.3.0",
    "preact-render-to-string": "^5.1.19",
    "preact-ssr-prepass": "^1.1.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.3.4",
    "wasm-brotli": "^2.0.2",
    "wasm-zopfli": "^1.0.2"
  }
}
