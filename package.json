{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:spa": "tsc && cross-env NODE_ENV=production vite build",
    "build:ssr": "tsc && cross-env BUILD_MODE=ssr NODE_ENV=production vite build --outDir=distSSR",
    "build:packages": "npm run build:ssr && npm run build:spa",
    "prerender": "node distSSR/ssr.cjs.js",
    "build": "npm run build:packages && npm run prerender",
    "serve": "vite preview",
    "subset": "node subset.mjs",
    "font-comp": "node font"
  },
  "dependencies": {
    "@svelte-navigator/history": "^0.4.0",
    "classnames": "^2.3.1",
    "preact": "^10.5.13"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "preact-render-to-string": "^5.1.19",
    "preact-ssr-prepass": "^1.1.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.2.4"
  }
}
